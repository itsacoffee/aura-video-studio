# API Health Check
VITE_API_HEALTH_CHECK_URL=http://localhost:5005/health/live
VITE_API_HEALTH_CHECK_TIMEOUT=30000

# Development Features
VITE_ENABLE_HOT_RELOAD=true
VITE_ENABLE_SOURCE_MAPS=true
